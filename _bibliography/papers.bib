	pages = {6385--6399},
	file = {Full Text PDF:C\:\\Users\\Bonne\\Zotero\\storage\\IW5QBLU8\\Ford et al. - 2019 - A low-cost monitor for measurement of fine particu.pdf:application/pdf},
