	file = {ACS Full Text Snapshot:C\:\\Users\\Bonne\\Zotero\\storage\\MHTJY928\\acs.est.html:text/html;Full Text PDF:C\:\\Users\\Bonne\\Zotero\\storage\\E3FN75WC\\Hammer et al. - 2020 - Global Estimates and Long-Term Trends of Fine Part.pdf:application/pdf},
