	urldate = {2021-10-12},
	journal = {GeoHealth},
	author = {Sorensen, Cecilia and House, John A. and O'Dell, Katelyn and Brey, Steven J. and Ford, Bonne and Pierce, Jeffrey R. and Fischer, Emily V. and Lemery, Jay and Crooks, James L.},
	month = may,
