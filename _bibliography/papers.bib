	author = {Cheeseman, M. and Ford, B. and Volckens, J. and Lyapustin, A. and Pierce, J. R.},
	year = {2020},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1029/2020GL088949},
	keywords = {wildfire smoke, particulate matter, plume injection height, satellite measurements},
	pages = {e2020GL088949},
	file = {Full Text PDF:C\:\\Users\\Bonne\\Zotero\\storage\\JRGPPWSF\\Cheeseman et al. - 2020 - The Relationship Between MAIAC Smoke Plume Heights.pdf:application/pdf;Snapshot:C\:\\Users\\Bonne\\Zotero\\storage\\4FZUF3XE\\2020GL088949.html:text/html},
}

@article{burkhardt_relationship_2020,
	title = {The relationship between monthly air pollution and violent crime across the {United} {States}},
