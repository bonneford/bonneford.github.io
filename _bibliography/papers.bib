	year = {2021},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1029/2021GL093405},
	keywords = {wildfires, satellite, acyl peroxynitrates, chemical production, transport, Western U.S.},
	pages = {e2021GL093405},
	file = {Full Text PDF:C\:\\Users\\Bonne\\Zotero\\storage\\NEVTLT5J\\Juncosa Calahorrano et al. - 2021 - Evolution of Acyl Peroxynitrates (PANs) in Wildfir.pdf:application/pdf;Snapshot:C\:\\Users\\Bonne\\Zotero\\storage\\TF6YXUIH\\2021GL093405.html:text/html},
